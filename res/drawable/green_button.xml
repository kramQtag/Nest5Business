<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#1F361F"
                android:endColor="#5D6E5D"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#9EA89E" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:endColor="#1F361F"
                android:startColor="#5D6E5D"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#9EA89E" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>